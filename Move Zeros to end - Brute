// Move all zeros to end of array - Brute
#include<bits/stdc++.h>
using namespace std;
void MoveZerosToEnd(vector<int> &arr,int n)
{   
    int temp[n];
    int j = 0;
    for(int i = 0 ; i < n ; i++){
        if(arr[i] != 0){
            temp[j] = arr[i];
            j++;
        }
    }
    while(j < n){
        temp[j++] = 0;
    }
    for(int i = 0 ; i < n ; i++){
        arr[i] = temp[i];
    }
}
int main()
{
    vector<int> arr = {1,2,0,1,0,4,0};
    MoveZerosToEnd(arr,arr.size());
    for(auto it: arr){
        cout<<it<<" ";
    }
}


// TC = O(N)
// SC = O(N)
