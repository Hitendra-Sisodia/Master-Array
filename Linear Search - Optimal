// Linear Search - Optimal
#include<bits/stdc++.h>
using namespace std;

int pushZerosToEnd(vector<int> &arr, int n , int k){
    for(int i = 0 ; i < n ; i++){
        if(arr[i] == k){
            return 1;
        }
    }
    return -1;
}
int main()
{   
    vector<int> arr = {3, 5, 74, 100, 4};
    int ans = pushZerosToEnd(arr,arr.size(), 4);
    if(ans == 1){
        cout<<"OP";
    }
    else{
        cout<<"Not OP";
    }
}


// TC = O(N)
// SC = O(1)
