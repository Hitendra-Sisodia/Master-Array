// Find the number that appears once, and the other numbers twice - Optimal (XOR)

#include<bits/stdc++.h>
using namespace std;
int AppearOnce(vector<int> &arr, int n)
{
    int XOR = 0;
    for(int i = 0 ; i < n ; i++){
        XOR ^= arr[i];
    }
    return XOR;
}
int main()
{
    vector<int> arr = {10,1,2,1,2,10,40};
    cout<<AppearOnce(arr,arr.size());
}
